func Test${2:FuncName}(t *testing.T) {
	t.Parallel()

	type args struct {
		content string
	}

	cases := map[string]struct {
		args      args
		want      bool
	}{
		"simple": {
			args{
				content: "test",
			},
			true,
		},
	}

	for intention, testCase := range cases {
		intention := intention
		testCase := testCase

		t.Run(intention, func(t *testing.T) {
			t.Parallel()

			if got := ${2:FuncName}(testCase.args.content); got != testCase.want {
				t.Errorf("${2:FuncName}() = %t, want %t", got, testCase.want)
			}
		})
	}
}

